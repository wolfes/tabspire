{
  "name": "Tabspire",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Command Line / GUI for saving & opening tabs.",
  "browser_action": {
    "default_icon": "img/tabscape16.png"
  },
  "background": {
    "page": "background/index.html"
  },
  "permissions": [
    "tabs"
  ],
  "omnibox": {
    "keyword": "t"
  },
  "options_page": "view/index.html",
  "update_url": "http://wstyke.com/tabspire/updates.xml"
}
